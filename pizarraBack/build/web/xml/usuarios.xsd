<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
   
    <xs:complexType name="user">
        <xs:attribute name="id" type="xs:integer"/>
        <xs:attribute name="nombreUsr" type="xs:string"/>
        <xs:attribute name="contrasena"   type="xs:string"/>
    </xs:complexType>
	
    <xs:element name="usuarios">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="usuario" type="user" maxOccurs="unbounded"/>			
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
